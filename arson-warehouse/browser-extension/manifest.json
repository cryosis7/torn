{
  "manifest_version": 2,
  "name": "ArsonWarehouse",
  "version": "4.0",
  "author": "Sulsay",
  "permissions": [
    "activeTab",
    "https://arsonwarehouse.com/"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": ["https://www.torn.com/*"],
      "js": ["js/content-scripts/torn/torn.js"],
      "run_at": "document_end"
    },
    {
      "matches": ["https://www.torn.com/trade.php"],
      "js": [
        "js/content-scripts/trade/TradeSide.js",
        "js/content-scripts/trade/TradeItem.js",
        "js/content-scripts/trade/TradeValueModal.js",
        "js/content-scripts/trade/StyleInjector.js",
        "js/content-scripts/trade/trade.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "js/page-scripts/collect-equipment-stats.js"
  ],
  "browser_action": {
  },
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}